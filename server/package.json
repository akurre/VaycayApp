{
  "name": "vaycay-graphql-server",
  "version": "1.0.0",
  "description": "GraphQL API for Vaycay weather data",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "generate": "tsx src/schema.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:pull": "prisma db pull",
    "import-data": "tsx scripts/import-data.ts",
    "import-csv-data": "tsx scripts/import-csv-data.ts",
    "aggregate-weekly-weather": "tsx scripts/aggregate-weekly-weather.ts",
    "check-db": "tsx scripts/check-db-state.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "graphql",
    "apollo",
    "prisma",
    "weather",
    "api"
  ],
  "author": "Ashlen Kurre",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@prisma/client": "^5.22.0",
    "@types/cors": "^2.8.19",
    "@types/node-cache": "^4.1.3",
    "@types/nodemailer": "^7.0.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.9.0",
    "nexus": "^1.3.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^5.0.5",
    "@types/express-serve-static-core": "^5.1.0",
    "@types/node": "^24.10.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.5.0",
    "prettier": "^3.4.2",
    "prisma": "^5.22.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.46.3"
  }
}
